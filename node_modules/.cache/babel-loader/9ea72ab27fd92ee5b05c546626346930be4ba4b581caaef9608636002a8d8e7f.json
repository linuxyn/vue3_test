{"ast":null,"code":"import { ref, reactive } from \"vue\";\nexport default {\n  name: \"App\",\n  setup() {\n    // 数据\n    /*\n    let name = ref(\"张三\");\n    let age = ref(18);\n    let job = reactive({\n      type: \"前端工程师\",\n      salary: \"30k\",\n      a:{\n        b:{\n          c:666\n        }\n      }\n    });\n    let hobby = reactive(['抽烟','喝酒','烫头']) */\n    let person = reactive({\n      job: {\n        name: \"张三\",\n        age: 18,\n        type: \"前端工程师\",\n        salary: \"30k\",\n        a: {\n          b: {\n            c: 666\n          }\n        }\n      },\n      hobby: ['抽烟', '喝酒', '烫头']\n    });\n    function changeInfo() {\n      person.name = \"李四\";\n      age.value = 19;\n      job.type = \"运维开发工程师\";\n      job.salary = \"50k\";\n      job.a.b.c = 99, hobby[0] = '学习';\n\n      // console.log(job);\n    }\n\n    return {\n      name,\n      age,\n      changeInfo,\n      job,\n      hobby\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","name","setup","person","job","age","type","salary","a","b","c","hobby","changeInfo","value"],"sources":["/Users/linuxyn/vue3_test/src/App.vue"],"sourcesContent":["<template>\n  <!-- Vue3组件中的模板结构中可以没有根标签 -->\n  <h2>姓名:{{ name }}</h2>\n  <h2>年龄:{{ age }}</h2>\n  <h2>年龄:{{ job.type }}</h2>\n  <h2>年龄:{{ job.salary }}</h2>\n  <h2>爱好:{{ hobby }}</h2>\n  <h2>测试数据c的值:{{ job.a.b.c }}</h2>\n  <button @click=\"changeInfo\">修改个人信息</button>\n</template>\n<script>\nimport { ref,reactive } from \"vue\";\nexport default {\n  name: \"App\",\n  setup() {\n    // 数据\n    /*\n    let name = ref(\"张三\");\n    let age = ref(18);\n    let job = reactive({\n      type: \"前端工程师\",\n      salary: \"30k\",\n      a:{\n        b:{\n          c:666\n        }\n      }\n    });\n    let hobby = reactive(['抽烟','喝酒','烫头']) */\n    let person = reactive({\n      job:{\n        name:\"张三\",\n        age:18,\n        type: \"前端工程师\",\n        salary: \"30k\",\n        a:{\n          b:{\n            c:666\n          }\n        }\n      },\n      hobby:['抽烟','喝酒','烫头']\n\n    })\n\n    function changeInfo() {\n      person.name = \"李四\";\n      age.value = 19;\n      job.type = \"运维开发工程师\";\n      job.salary = \"50k\";\n      job.a.b.c = 99,\n      hobby[0] = '学习'\n\n      // console.log(job);\n    }\n\n    return {\n      name,\n      age,\n      changeInfo,\n      job,\n      hobby\n    };\n  },\n};\n</script>\n\n<style>\n</style>\n"],"mappings":"AAWA,SAASA,GAAG,EAACC,QAAO,QAAS,KAAK;AAClC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,KAAKA,CAAA,EAAG;IACN;IACA;;;;;;;;;;;;;IAaA,IAAIC,MAAK,GAAIH,QAAQ,CAAC;MACpBI,GAAG,EAAC;QACFH,IAAI,EAAC,IAAI;QACTI,GAAG,EAAC,EAAE;QACNC,IAAI,EAAE,OAAO;QACbC,MAAM,EAAE,KAAK;QACbC,CAAC,EAAC;UACAC,CAAC,EAAC;YACAC,CAAC,EAAC;UACJ;QACF;MACF,CAAC;MACDC,KAAK,EAAC,CAAC,IAAI,EAAC,IAAI,EAAC,IAAI;IAEvB,CAAC;IAED,SAASC,UAAUA,CAAA,EAAG;MACpBT,MAAM,CAACF,IAAG,GAAI,IAAI;MAClBI,GAAG,CAACQ,KAAI,GAAI,EAAE;MACdT,GAAG,CAACE,IAAG,GAAI,SAAS;MACpBF,GAAG,CAACG,MAAK,GAAI,KAAK;MAClBH,GAAG,CAACI,CAAC,CAACC,CAAC,CAACC,CAAA,GAAI,EAAE,EACdC,KAAK,CAAC,CAAC,IAAI,IAAG;;MAEd;IACF;;IAEA,OAAO;MACLV,IAAI;MACJI,GAAG;MACHO,UAAU;MACVR,GAAG;MACHO;IACF,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}