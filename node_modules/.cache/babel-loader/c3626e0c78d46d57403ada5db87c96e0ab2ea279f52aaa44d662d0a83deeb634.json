{"ast":null,"code":"import { ref, reactive } from \"vue\";\nexport default {\n  name: \"App\",\n  setup() {\n    // 数据\n    let person = reactive({\n      name: \"张三\",\n      age: 18,\n      job: {\n        type: \"前端工程师\",\n        salary: \"30k\",\n        a: {\n          b: {\n            c: 666\n          }\n        }\n      },\n      hobby: ['抽烟', '喝酒', '烫头']\n    });\n    let num = 100;\n    function changeInfo() {\n      person.name = \"李四\";\n      person.age = 19;\n      person.job.type = \"运维开发工程师\";\n      person.job.salary = \"50k\";\n      person.job.a.b.c = 99, person.hobby[0] = '学习';\n      // console.log(job);\n    }\n\n    function addGender() {\n      person.gender = '男';\n    }\n    function deleteName() {\n      delete person.name;\n    }\n    return {\n      person,\n      changeInfo,\n      addGender,\n      deleteName,\n      num\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","name","setup","person","age","job","type","salary","a","b","c","hobby","num","changeInfo","addGender","gender","deleteName"],"sources":["/Users/linuxyn/vue3_test/src/App.vue"],"sourcesContent":["<template>\n  <!-- Vue3组件中的模板结构中可以没有根标签 -->\n  <h2 v-show=\"person.name\">姓名:{{ person.name }}</h2>\n  <h2>年龄:{{ person.age }}</h2>\n  <h2 v-show=\"person.gender\">性别:{{ person.gender }}</h2>\n  <h2>年龄:{{ person.age }}</h2>\n  <h2>职业:{{ person.job.type }}</h2>\n  <h2>薪资:{{ person.job.salary }}</h2>\n  <h2>爱好:{{ person.hobby }}</h2>\n  <h2 v-show>num的值: {{ num }}</h2>\n  <h2>测试数据c的值:{{ person.job.a.b.c }}</h2>\n  <button @click=\"changeInfo\">修改个人信息</button>\n  <button @click=\"addGender\">增加一个性别属性</button>\n  <button @click=\"deleteName\">删除一个name的属性</button>\n</template>\n<script>\nimport { ref,reactive } from \"vue\";\nexport default {\n  name: \"App\",\n  setup() {\n    // 数据\n  let person = reactive({\n      name:\"张三\",\n      age:18,\n      job:{\n        type: \"前端工程师\",\n        salary: \"30k\",\n        a:{\n          b:{\n            c:666\n          }\n        }\n      },\n      hobby:['抽烟','喝酒','烫头']\n\n    })\n    let num = 100\n\n    function changeInfo() {\n      person.name = \"李四\";\n      person.age = 19;\n      person.job.type = \"运维开发工程师\";\n      person.job.salary = \"50k\";\n      person.job.a.b.c = 99,\n      person.hobby[0] = '学习'\n      // console.log(job);\n    }\n    function addGender(){\n      person.gender = '男'\n    }\n    function deleteName(){\n      delete person.name\n    }\n    return {\n      person,\n      changeInfo,\n      addGender,\n      deleteName,\n      num\n    };\n  },\n};\n</script>\n\n<style>\n</style>\n"],"mappings":"AAgBA,SAASA,GAAG,EAACC,QAAO,QAAS,KAAK;AAClC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,KAAKA,CAAA,EAAG;IACN;IACF,IAAIC,MAAK,GAAIH,QAAQ,CAAC;MAClBC,IAAI,EAAC,IAAI;MACTG,GAAG,EAAC,EAAE;MACNC,GAAG,EAAC;QACFC,IAAI,EAAE,OAAO;QACbC,MAAM,EAAE,KAAK;QACbC,CAAC,EAAC;UACAC,CAAC,EAAC;YACAC,CAAC,EAAC;UACJ;QACF;MACF,CAAC;MACDC,KAAK,EAAC,CAAC,IAAI,EAAC,IAAI,EAAC,IAAI;IAEvB,CAAC;IACD,IAAIC,GAAE,GAAI,GAAE;IAEZ,SAASC,UAAUA,CAAA,EAAG;MACpBV,MAAM,CAACF,IAAG,GAAI,IAAI;MAClBE,MAAM,CAACC,GAAE,GAAI,EAAE;MACfD,MAAM,CAACE,GAAG,CAACC,IAAG,GAAI,SAAS;MAC3BH,MAAM,CAACE,GAAG,CAACE,MAAK,GAAI,KAAK;MACzBJ,MAAM,CAACE,GAAG,CAACG,CAAC,CAACC,CAAC,CAACC,CAAA,GAAI,EAAE,EACrBP,MAAM,CAACQ,KAAK,CAAC,CAAC,IAAI,IAAG;MACrB;IACF;;IACA,SAASG,SAASA,CAAA,EAAE;MAClBX,MAAM,CAACY,MAAK,GAAI,GAAE;IACpB;IACA,SAASC,UAAUA,CAAA,EAAE;MACnB,OAAOb,MAAM,CAACF,IAAG;IACnB;IACA,OAAO;MACLE,MAAM;MACNU,UAAU;MACVC,SAAS;MACTE,UAAU;MACVJ;IACF,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}