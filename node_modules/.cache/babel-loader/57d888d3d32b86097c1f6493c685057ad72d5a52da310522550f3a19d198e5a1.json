 18\n    let a = 100\n\n    // 方法\n    function sayHello() {\n      alert(`我叫${name},我今年${age},你好啊`)\n    }\n    function test2() { \n      console.log(name)\n      console.log(age)\n      console.log(sayHello)\n      console.log(this.gender)\n      console.log(this.sayWelcome)\n     }\n    return {\n      name,\n      age,\n      sayHello,\n      test2\n    }\n    // 返回一个函数（渲染函数，和Vue2中的render类似）\n    // return ()=> h('h1', '尚硅谷')\n  }\n}\n</script>\n\n<style>\n/* #app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n} */\n</style>\n"],"mappings":"AAiBA;AACA,eAAe;EACbA,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,GAAG;MACXC;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAE;MACVC,KAAK,CAAC,WAAW;IACnB,CAAC;IACDC,KAAKA,CAAA,EAAE;MACLC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACP,MAAM;MACvBM,OAAO,CAACC,GAAG,CAAC,IAAI,CAACT,IAAI;MACrBQ,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,GAAG;MACpBF,OAAO,CAACC,GAAG,CAAC,IAAI,CAACJ,UAAU;MAC3BG,OAAO,CAACC,GAAG,CAAC,IAAI,CAACE,QAAQ;IAC3B;EACF,CAAC;EAGD;EACAC,KAAKA,CAAA,EAAG;IACN;IACA,IAAIZ,IAAG,GAAI,IAAG;IACd,IAAIU,GAAE,GAAI,EAAC;IACX,IAAIP,CAAA,GAAI,GAAE;;IAEV;IACA,SAASQ,QAAQA,CAAA,EAAG;MAClBL,KAAK,CAAE,KAAIN,IAAK,OAAMU,GAAI,MAAK;IACjC;IACA,SAASG,KAAKA,CAAA,EAAG;MACfL,OAAO,CAACC,GAAG,CAACT,IAAI;MAChBQ,OAAO,CAACC,GAAG,CAACC,GAAG;MACfF,OAAO,CAACC,GAAG,CAACE,QAAQ;MACpBH,OAAO,CAACC,GAAG,CAAC,IAAI,CAACP,MAAM;MACvBM,OAAO,CAACC,GAAG,CAAC,IAAI,CAACJ,UAAU;IAC5B;IACD,OAAO;MACLL,IAAI;MACJU,GAAG;MACHC,QAAQ;MACRE;IACF;IACA;IACA;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}