ry = \"50k\";\n      person.job.a.b.c = 99,\n      person.hobby[0] = '学习'\n      // console.log(job);\n    }\n    function addGender(){\n      console.log('1')\n      person.gender = '男'\n    }\n\n    return {\n      person,\n      changeInfo,\n      add\n    };\n  },\n};\n</script>\n\n<style>\n</style>\n"],"mappings":"AAcA,SAASA,GAAG,EAACC,QAAO,QAAS,KAAK;AAClC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,KAAKA,CAAA,EAAG;IACN;IACF,IAAIC,MAAK,GAAIH,QAAQ,CAAC;MAClBC,IAAI,EAAC,IAAI;MACTG,GAAG,EAAC,EAAE;MACNC,GAAG,EAAC;QACFC,IAAI,EAAE,OAAO;QACbC,MAAM,EAAE,KAAK;QACbC,CAAC,EAAC;UACAC,CAAC,EAAC;YACAC,CAAC,EAAC;UACJ;QACF;MACF,CAAC;MACDC,KAAK,EAAC,CAAC,IAAI,EAAC,IAAI,EAAC,IAAI;IAEvB,CAAC;IAED,SAASC,UAAUA,CAAA,EAAG;MACpBT,MAAM,CAACF,IAAG,GAAI,IAAI;MAClBE,MAAM,CAACC,GAAE,GAAI,EAAE;MACfD,MAAM,CAACE,GAAG,CAACC,IAAG,GAAI,SAAS;MAC3BH,MAAM,CAACE,GAAG,CAACE,MAAK,GAAI,KAAK;MACzBJ,MAAM,CAACE,GAAG,CAACG,CAAC,CAACC,CAAC,CAACC,CAAA,GAAI,EAAE,EACrBP,MAAM,CAACQ,KAAK,CAAC,CAAC,IAAI,IAAG;MACrB;IACF;;IACA,SAASE,SAASA,CAAA,EAAE;MAClBC,OAAO,CAACC,GAAG,CAAC,GAAG;MACfZ,MAAM,CAACa,MAAK,GAAI,GAAE;IACpB;IAEA,OAAO;MACLb,MAAM;MACNS,UAAU;MACVK;IACF,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}